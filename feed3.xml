---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title type="text" xml:lang="en">{{ site.desc }}</title>
	<link type="application/atom+xml" href="http://morningpulp.com/jekyll/feed3.xml" rel="self"/>
 	<link type="text" href="{{ site.url }}" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.url }}</id>
	<author>
		<name>{{ post.author }}</name>
	</author>
	<rights>Copyright &copy; {% assign copyYear = '2014' %}{% assign curYear = 'now' | date: "%Y" %}{{ copyYear }}{% if copyYear != curYear %}-{{ curYear }}{% else %}{% endif %}</a> Cameron Tracy</rights>
	
	{% for post in site.posts limit:20 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="{{ site.url}}{{ site.baseurl }}{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>{{ site.url }}{{ base.url }}{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>