---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title | xml_escape }}</title>
  <link rel="self" type="application/atom+xml" href="{{ site.url }}{{ site.baseurl }}/atom.xml" />
  <id>{{ site.url }}{{ site.baseurl }}/atom.xml</id>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <rights>Copyright © {% assign copyYear = ‘2014’ %}{% assign curYear = ‘now’ | date: “%Y” %}{{ copyYear }}{% if copyYear != curYear %}-{{ curYear }}{% endif %} Cameron Tracy</rights>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link rel="alternate" type="text/html" href="{{ site.url }}{{ site.baseurl }}{{ post.url }}" />
    <id>{{ site.url }}{{ site.baseurl }}{{ post.url }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author><name>Cameron Tracy</name></author>
    <content>{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>