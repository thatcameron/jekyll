---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title | xml_escape }}</title>
  <link rel="self" type="application/atom+xml" href="{{ site.url }}{{ site.baseurl }}/index.xml" />
  <id>{{ site.url }}{{ site.baseurl }}/atom.xml</id>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <rights>Copyright Â© {% assign copyYear = "2014" %}{% assign curYear = site.time | date: "%Y" %}{{ copyYear }}{% if copyYear != curYear %}-{{ curYear }}{% endif %} {{ site.author }}</rights>
  {% for post in site.posts limit:10 %}<entry>
    <title>{{ post.title | xml_escape }}</title>
    {% if post.link %}<link rel="alternate" type="text/html" href="{{ post.link }}" />
    <link rel="related"{% else %}<link rel="alternate"{% endif %} type="text/html" href="{{ site.url }}{{ site.baseurl }}{{ post.url }}" />
    <id>tag:{{ site.url | remove: "http://" }},{{ post.date | date: "%Y-%m-%d" }}:{{ site.baseurl }}{{ post.url }}</id>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ site.author }}</name>
      <uri>{{ site.url }}</uri>
    </author>
    <content>
      {{ post.content | xml_escape }}
    </content>
  </entry>{% endfor %}
</feed>